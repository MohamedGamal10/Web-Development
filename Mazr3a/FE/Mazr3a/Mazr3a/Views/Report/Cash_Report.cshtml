<br />
<div style="border:1px solid black; padding:10px;">
    <h6>الاجمالي : @ViewBag.Total جنية </h6>
</div>

<br/>
<div>
    <h5>النقدية</h5>
    <table class="table" style="border:1px solid black;">
        <thead>
           <tr>
            <th>البند</th>
            <th>اليوم</th>
            <th>السعر</th>
           </tr>
        </thead>
        <tbody>
@foreach (var item in ViewBag.Cash)
        {
           <tr>
            <td>@item.item_Name</td>
            <td>@item.Date.ToString("MM/dd/yyyy")</td>
            <td>@item.Price</td>
           </tr>
    }
        </tbody>
    </table>
</div>